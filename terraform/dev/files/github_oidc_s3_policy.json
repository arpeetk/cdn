{ 
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
            "s3:PutObject",
            "s3:ListBucketVersions",
            "s3:ListBucket"
        ],
        "Resource": [
            "arn:aws:s3:::cdn-skiff/*",
            "arn:aws:s3:::cdn-skiff"
        ]
    },
    {
        "Effect": "Allow",
        "Action": "s3:ListAllMyBuckets",
        "Resource": "*"
    }
  ]
}